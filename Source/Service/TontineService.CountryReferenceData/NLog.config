<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <targets>
    <target name="logfile" xsi:type="File" fileName="${basedir}/logs/CountryRefDataService_${shortdate}.log" 
            layout="${date:format=s}|${level}|${message}" />
  </targets>
  <rules>
    <logger name="CountryRefDataService" minlevel="Info" writeTo="logfile" />
  </rules>
</nlog>